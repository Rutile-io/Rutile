        "ethereumjs-tx": "^2.1.1",
        "ethereumjs-util": "^6.2.0",
        "ethereumjs-vm": "^4.1.0",
        "ethers": "^4.0.39",
        "go-ipfs-dep": "^0.4.22",
        "ipfs-mini": "^1.1.5",
        "ipfsd-ctl": "^0.47.4",
        "isolated-vm": "^2.0.1",
        "js-logger": "^1.6.0",
        "keccak": "^1.4.0",
        "memorystream": "^0.3.1",
        "merkle-patricia-tree": "^3.0.0",
        "node-fetch": "^2.6.1",
        "pouchdb": "^7.1.1",
        "pouchdb-find": "^7.1.1",
        "pouchdb-replication-stream": "^1.2.9",
