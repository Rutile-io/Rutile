{
    "name": "rutile",
    "version": "1.0.0",
    "description": "Decentralised hosting",
    "main": "index.js",
    "scripts": {
        "build:node": "cross-env NODE_ENV=development webpack --target node --config ./webpack.config.js",
        "build": "cross-env NODE_ENV=development webpack --config ./webpack.config.js",
        "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
        "docs": "documentation build ./src > ./docs/codebase.md --shallow false --format md --sort-order alpha",
        "release": "standard-version",
        "start": "npm run build:watch:node & nodemon --experimental-worker ./build/rutile.js localhost 8932 --watch ./build/rutile.js",
        "test": "jest",
        "lint": "eslint ./src/ --cache --ignore-pattern .gitignore",
        "build:watch:node": "cross-env NODE_ENV=development webpack --watch --target node --config ./webpack.config.js",
        "start:node": "npm run build:node && node ./build/rutile.js"
    },
    "author": "Franklin Waller",
    "license": "MIT",
    "devDependencies": {
        "@types/bn.js": "^4.11.5",
        "@types/crypto-js": "^3.1.43",
        "@types/levelup": "^3.1.1",
        "@types/node-fetch": "^2.5.3",
        "@types/pouchdb": "^6.4.0",
        "@types/text-encoding": "0.0.35",
        "@types/toposort": "^2.0.3",
        "arraybuffer-loader": "^1.0.8",
        "babel-core": "6.26.3",
        "babel-eslint": "10.0.3",
        "babel-loader": "8.0.6",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
        "babel-plugin-transform-es3-property-literals": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "cross-env": "6.0.3",
        "documentation": "12.1.3",
        "eslint-summary": "^1.0.0",
        "jest": "^24.9.0",
        "nodemon": "^1.19.4",
        "standard-version": "^7.0.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.5.0",
        "typescript": "^3.7.2",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0"
    },
